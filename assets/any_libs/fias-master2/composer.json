{
    "name": "marvin255/fias",
    "description": "fias parser for php",
    "type": "library",
    "keywords": ["php", "fias"],
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.5",
        "phpunit/dbunit": "~3.0",
        "mockery/mockery": "~1.0",
        "fzaninotto/faker": "~1.7",
        "friendsofphp/php-cs-fixer": "~2.11",
        "sebastian/phpcpd": "~3.0",
        "phpmetrics/phpmetrics": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "marvin255\\fias\\": "src/",
            "marvin255\\fias\\tests\\": "tests/src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/marvin255/fias"
        }
    ]
}
